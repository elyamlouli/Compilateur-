    {
        int n;
        int s;
        int r;
        n = 10;
        s = 0;
        for i = 1, n
        {
            for j = 1, i
            {
                for k = 1, j
                {
                    s += 1;
                }
            }
        }
        r = n * (n + 1) * (n + 2) / 1 / 2 / 3;
        WriteInt (s);
        WriteString ("=");
